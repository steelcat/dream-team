include blocks/_blocks
doctype html
html(lang="ru")
    head
        meta(charset='utf-8')
        link(href='css/style.css' rel='stylesheet')
    +page
        +wrapper
            +content
                +block-result
            +sidebar
                +block-upload
                +block-place
                +block-transparency
                +block-download
        +footer
        script(src="../bower_components/jquery/dist/jquery.min.js")
        script(src="../bower_components/jquery-ui/ui/widget.js")
        script(src="../bower_components/jquery-file-upload/js/jquery.iframe-transport.js")
        script(src="../bower_components/jquery-file-upload/js/jquery.fileupload.js")
        script(src="../app/js/main.js")